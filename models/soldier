
from sqlmodel import Field, SQLModel 

class Soldier:
    def __init__(self, id: int, first_name: str, last_name: str, gender: str, city: str, distance_in_km: int, status: bool):
        self.id = id
        self.first_name = first_name
        self.last_name = last_name
        self.gender = gender
        self.city = city
        self.distance_in_km = distance_in_km
        self.status = status




